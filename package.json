  },
  "homepage": "https://github.com/vuejs/vuex-router-sync#readme",
  "peerDependencies": {
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "buble": "^0.16.0",
    "jest": "^21.2.1",
    "typescript": "^2.5.3",
    "vue": "^2.5.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0"
  }
